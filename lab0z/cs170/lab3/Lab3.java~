import java.util.Scanner;

public class Lab3{
	


	public static void escape(){
		System.out.println("I said \"Hello\" to James.");
		System.out.print("The string \"\" is an empty string");
		System.out.print("\n");
		System.out.print("Shaq is 7\'1\" tall.");
		System.out.print("\n");
		System.out.println("Comments can begin with // but not \\\\.");
	}

	public static void convert(){
		Scanner in= new Scanner(System.in);
		System.out.println("Please enter the number of miles:");
		double x= in.nextInt();
		System.out.println("You entered: " +x);
	}

	public static void main(String[] args){
		//escape();
		convert();
	
	}

}
